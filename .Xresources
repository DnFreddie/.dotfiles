xterm*faceName: JetBrains Mono:size=15 
!XTerm.vt100.faceName: Monospace::antialias=true:size=15
XTerm.vt100.locale: false
XTerm.vt100.utf8: true
XTerm*selectToClipboard:true
XTerm*SaveLines: 2000
XTerm*rightScrollBar: flase
XTerm*ScrollBar: false
XTerm*scrollTtyOutput: false
XTerm.termName: xterm-256color
xterm*metaSendsEscape: true
XTerm*vt100.translations: #override \n\
  Ctrl <Key>-: smaller-vt-font() \n\
  Ctrl <Key>+: larger-vt-font() \n\
  Ctrl <Key>0: set-vt-font(d) \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD)
! Background and Foreground
*.background: black
*.foreground: #F8F8F2

! Base Colors
! Black
*.color0:    #000000
! Red
*.color1:    #FF5555
! Green
*.color2:    #00FF00
! Yellow
*.color3:    #F1FA8C
! Blue
*.color4:    #BD93F9
! Magenta
*.color5:    #FF79C6
! Cyan
*.color6:    #8BE9FD
! White
*.color7:    #BFBFBF

! Bright Colors
! Bright Black (Gray)
*.color8:    #4D4D4D
! Bright Red
*.color9:    #FF6E67
! Bright Green
*.color10:   #CCFFCC
! Bright Yellow
*.color11:   #F4F99D
! Bright Blue
*.color12:   #CAA9FA
! Bright Magenta
*.color13:   #FF92D0
! Bright Cyan
*.color14:   #9AEDFE
! Bright White
*.color15:   #E6E6E6
! Smoother resizing settings
xterm*resizeGravity: SouthEast
xterm*dynamicColors: true
xterm*autoWrap: true
xterm*scaleHeight: 1.01
xterm*faceSizeStep: 0.6
xterm*allowWindowOps: true
*loginShell: true
*geometry: 100
*TrimSelection: true
*charClass: 33:48,37:48,43-47:48,64:48


